{
  "exclude": [
    "node_modules",
  ],
  "include": [
    "index.ts"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019"
    ],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "target": "ES2019",
    "module": "CommonJS",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "declaration": true,
    "jsx": "react-jsx",
    "importHelpers": true
  }
}